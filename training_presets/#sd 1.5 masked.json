{
    "training_method": "FINE_TUNE",
    "debug_mode": false,
    "debug_dir": "debug",
    "workspace_dir": "workspace/run",
    "cache_dir": "workspace-cache/run",
    "tensorboard": true,
    "model_type": "STABLE_DIFFUSION_15",
    "base_model_name": "runwayml/stable-diffusion-v1-5",
    "extra_model_name": "",
    "weight_dtype": "FLOAT_32",
    "output_dtype": "FLOAT_32",
    "output_model_format": "SAFETENSORS",
    "output_model_destination": "models/model.safetensors",
    "concept_file_name": "training_concepts/concepts.json",
    "circular_mask_generation": false,
    "random_rotate_and_crop": false,
    "aspect_ratio_bucketing": true,
    "latent_caching": true,
    "latent_caching_epochs": 1,
    "clear_cache_before_training": true,
    "optimizer": "ADAMW",
    "learning_rate_scheduler": "CONSTANT",
    "learning_rate": 3e-06,
    "learning_rate_warmup_steps": 200,
    "learning_rate_cycles": 1,
    "weight_decay": 0.01,
    "epochs": 100,
    "batch_size": 4,
    "gradient_accumulation_steps": 1,
    "ema": "OFF",
    "ema_decay": 0.999,
    "ema_update_step_interval": 5,
    "train_text_encoder": true,
    "train_text_encoder_epochs": 30,
    "text_encoder_learning_rate": 3e-06,
    "text_encoder_layer_skip": 0,
    "train_unet": true,
    "train_unet_epochs": 100000,
    "unet_learning_rate": 3e-06,
    "offset_noise_weight": 0.05,
    "rescale_noise_scheduler_to_zero_terminal_snr": false,
    "force_v_prediction": false,
    "force_epsilon_prediction": false,
    "train_device": "cuda",
    "temp_device": "cpu",
    "train_dtype": "FLOAT_16",
    "only_cache": false,
    "resolution": 512,
    "masked_training": true,
    "unmasked_probability": 0.1,
    "unmasked_weight": 0.1,
    "normalize_masked_area_loss": true,
    "max_noising_strength": 0.8,
    "token_count": 1,
    "initial_embedding_text": "*",
    "lora_rank": 16,
    "lora_alpha": 1.0,
    "attention_mechanism": "XFORMERS",
    "sample_definition_file_name": "training_samples/samples.json",
    "sample_after": 10,
    "sample_after_unit": "MINUTE",
    "backup_after": 30,
    "backup_after_unit": "MINUTE",
    "backup_before_save": true
}